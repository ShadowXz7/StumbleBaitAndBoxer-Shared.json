let User = {
  Username: "Fire7 KnG",
  Skins: [],
  RewardID: ""
};

let Shared = {
  Skins_v4: [
    { SkinID: 1, Name: "Default" },
    { SkinID: 2, Name: "Ninja" },
    { SkinID: 3, Name: "Robot" }
  ]
};

function setNickname() {
  if (User.Username === "Fire7 KnG") {
    User.Username = "<color=red>ShadowXz<#FDD700><sup>[DEV]";
  }
}

function getRewardID() {
  return "activate_emotes";
}

function activateCosmetics() {
  Shared.Skins_v4.forEach(sharedSkin => {
    User.Skins.push(sharedSkin.SkinID);
  });

  let rewardID = getRewardID();
  let buffer = Buffer.from(rewardID, "utf-8");
  User.RewardID = buffer.toString("base64");
}

setNickname();
activateCosmetics();

console.log("Usuário atualizado:");
console.log(User);
String[] partyMembers = new String[100];
int count = 0;

void addMember(String player) {
    if (!isMember(player) && count < partyMembers.length) {
        partyMembers[count++] = player;
        System.out.println(player + " has joined the party.");
    } else if (isMember(player)) {
        System.out.println(player + " is already in the party.");
    }
}

void removeMember(String player) {
    for (int i = 0; i < count; i++) {
        if (partyMembers[i].equals(player)) {
            for (int j = i; j < count - 1; j++) {
                partyMembers[j] = partyMembers[j + 1];
            }
            partyMembers[--count] = null;
            System.out.println(player + " has left the party.");
            return;
        }
    }
    System.out.println(player + " is not in the party.");
}

boolean isMember(String player) {
    for (int i = 0; i < count; i++) {
        if (partyMembers[i].equals(player)) return true;
    }
    return false;
}

void listMembers() {
    System.out.println("Party Members:");
    for (int i = 0; i < count; i++) {
        System.out.println("- " + partyMembers[i]);
    }
}

addMember("Alice");
addMember("Bob");
listMembers();
removeMember("Alice");
listMembers();
